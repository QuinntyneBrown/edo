{
  "Logging": {
    "LogLevel": {
      "Default": "Warning"
    },
    "Sentry": {
      "Endpoint": "HTDC_EDO_SENTRY_ENDPOINT"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "Edo": "Server={0};Port={1};Database=edo;Userid={2};Password={3};"
  },
  "Edo": {
    "Database": {
      "Options": "edo/connection-string"
    },
    "Google": {
      "Endpoint": "https://maps.googleapis.com/maps/api/",
      "Options": "edo/google"
    },
    "Email": {
      "Options": "edo/email",
      "ApiKey": "apiKey",
      "SenderAddress": "senderAddress",
      "CustomerInvitationTemplateId": "invitationTemplateId",
      "AdministratorInvitationTemplateId": "adminInvitationTemplateId",
      "ExternalPaymentsTemplateId": "externalPaymentsTemplateId"
    },
    "Payfort": {
      "Options": "edo/payfort",
      "Urls": "edo/payfort/urls"
    }
  },
  "DataProviders": {
    "NetstormingConnector": "https://netstormingconnector-api.dev.happytravel.com/api/1.0/"
  },
  "Vault": {
    "Endpoint": "HTDC_VAULT_ENDPOINT",
    "Engine": "secrets",
    "Role": "edo",
    "Token": "HTDC_VAULT_TOKEN"
  },
  "Authority": {
    "Url": "https://identity.dev.happytravel.com/"
  },
  "PaymentLinks" : {
    "ServiceTypes" : { "HTL" : "Hotel booking", "TRN" : "Transfer"},
    "Currencies" : ["AED", "USD"],
    "BaseUrl": "https://dev.happytravel.com/pay"
  }
}
